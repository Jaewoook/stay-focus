{
    "manifest_version": 2,
    "name": "Stay FOCUS",
    "version": "0.0.1",
    "description": "Stay focus your main thing",
    "default_locale": "ko",
    "author": "Jaewook Ahn",
    "permissions": [
        "storage",
        "tabs"
    ],
    "background": {
        "scripts": ["build/index.js"],
        "persistent": false
    },
    "browser_action": {
        "default_popup": "build/settings/index.html"
    }
}
