{
    "manifest_version": 2,
    "name": "Stay FOCUS",
    "version": "0.0.1",
    "description": "Stay focus your main thing",
    "author": "Jaewook Ahn",
    "permissions": [
        "storage",
        "tabs"
    ],
    "web_accessible_resources": [
        "build/extension/images/*.png"
    ],
    "content_scripts": [
        {
            "matches": ["*://*/*"],
            "js": ["build/extension/index.js"]
        }
    ],
    "browser_action": {
        "default_popup": "build/settings/index.html"
    }
}
